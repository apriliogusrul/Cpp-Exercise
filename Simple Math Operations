/* Input two integers number A and B, calculate the operations result
  A + B
  A - B
  A * B
  A / B
  A % B
*/

#include <iostream>
using namespace std;
int main(){
    int A;
    int B;
    
    cin >> A >> B;

    cout<<"A + B = "<<A+B<<endl;
    cout<<"A - B = "<<A-B<<endl;
    cout<<"A * B = "<<A*B<<endl;
    cout<<"A / B = "<<A/B<<endl;
    cout<<"A % B = "<<A%B<<endl;

  return 0;
}
